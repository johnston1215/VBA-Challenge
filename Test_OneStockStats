Sub Test1()

Dim Opendate As Long
Dim Closedate As Long
Dim OpenPrice As Double
Dim ClosePrice As Double

Opendate = 20160101
Closedate = 20161230
Testrow = 263
Ticker = "A"
Range("i3") = Ticker
Volume = 0

    For d = 2 To Testrow
    TradeDate = Cells(d, 2).Value
'---Find Open and Close prices
    If TradeDate = Opendate Then
        OpenPrice = Cells(d, 3).Value
      ElseIf TradeDate = Closedate Then
        ClosePrice = Cells(d, 6).Value
    End If
    
'---Calculate Annual Change and Percentage Change
AnnualChange = ClosePrice - OpenPrice
Range("J3") = AnnualChange
Percentchange = AnnualChange / OpenPrice
Range("k3") = Percentchange
       
'---Calculation for total stock volume
    Volume = Volume + Cells(d, 7).Value
       
    Next d
    
'---Present results
Range("J4").Value = OpenPrice
Range("J5").Value = ClosePrice
Range("L3").Value = Volume


End Sub
